{
  "name": "Nife",
  "displayName": "Nife",
  "expo": {
    "name": "Nife",
    "slug": "Nife",
    "icon": "./src/media/Images/UpdatedLogoN.png",
    "privacy": "unlisted",
    "version": "1.0.0",
    "entryPoint": "node_modules/expo/AppEntry.js",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "image": "./src/media/Images/updatedLogo.png",
      "backgroundColor": "#301E48",
      "resizeMode": "contain"
    },
    "ios": {
      "bundleIdentifier": "com.nife.mobile",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.nife.mobile",
      "userInterfaceStyle": "dark",
      "versionCode": 1
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  }
}
